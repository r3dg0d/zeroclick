    "build": "microbundle --no-sourcemap",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiomotio/zeroclick.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
