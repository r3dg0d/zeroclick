{
  "name": "@studiomotio/zeroclick",
  "version": "1.1.0",
  "description": "A web concept for a zero click user experience",
  "source": "src/zeroclick.js",
  "main": "dist/zeroclick.js",
  "browser": "dist/zeroclick.umd.js",
  "unpkg": "dist/zeroclick.umd.js",
  "module": "dist/zeroclick.mjs",
  "scripts": {
    "lint": "eslint \"**/*.js\" && stylelint \"docs/src/**/*.scss\"",
    "dev": "webpack serve --config webpack.dev.js --progress",
    "docs": "webpack --config webpack.docs.js --progress",
    "build": "microbundle --no-sourcemap",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiomotio/zeroclick.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "web",
    "concept",
    "experiment",
    "user",
    "experience",
    "zeroclick"
  ],
  "author": {
    "name": "Studio MOTIO",
    "email": "hello@studiomotio.com",
    "url": "https://www.studiomotio.com",
    "github": "@studiomotio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/studiomotio/zeroclick/issues"
  },
  "homepage": "https://github.com/studiomotio/zeroclick#readme",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@barba/core": "^2.9.7",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.21.0",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "^6.2.0",
    "gsap": "^3.6.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.7",
    "postcss-loader": "^5.1.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
