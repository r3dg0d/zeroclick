    "concept",
    "experiment",
    "user",
    "experience",
    "zeroclick"
  ],
  "author": {
    "name": "Studio MOTIO",
    "email": "hello@studiomotio.com",
    "url": "https://www.studiomotio.com",
    "github": "@studiomotio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/studiomotio/zeroclick/issues"
  },
  "homepage": "https://github.com/studiomotio/zeroclick#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@barba/core": "^2.9.7",
    "@barba/css": "^2.1.15",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.13.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "microbundle": "^0.12.4",
    "mini-css-extract-plugin": "^1.3.0",
    "postcss": "^8.1.6",
    "postcss-loader": "^4.0.4",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  }
